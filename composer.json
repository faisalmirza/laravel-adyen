{
    "name": "ranium/laravel-adyen",
    "description": "A Laravel package for Adyen payment platform",
    "keywords": ["adyen", "laravel", "payment"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abbas Ali",
            "email": "abbas@ranium.in",
            "homepage": "http://ranium.in"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.4",
        "adyen/php-api-library": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Ranium\\LaravelAdyen\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ranium\\LaravelAdyen\\AdyenServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
